<%= search_form_for @q, class:"w-25 m-auto text-center",url: :root do |f| %>
<%= f.label :tags,"Search By Tags",class:"mb-2 fw-bold "%>
<%= f.search_field :tags_album_tags_cont, class: 'form-control mb-2 ' %>
